<%= form_for([:admin, @trip]) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :price %>
  <%= f.text_field :price %>
  <%= f.label :image_url %>
  <%= f.text_field :image_url %>
  <%= f.collection_select(:category_ids, @categories, :id, :title) %>
<%= f.submit %>
<% end %>

